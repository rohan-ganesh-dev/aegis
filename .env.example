# Environment Variables for Aegis

# Logging
LOG_LEVEL=INFO

GOOGLE_API_KEY= 

# Gemini Model Configuration
GEMINI_SUPERVISOR_MODEL=gemini-2.5-flash
GEMINI_SPECIALIST_MODEL=gemini-2.0-flash

# Vector DB Configuration (Mock)
VECTOR_DB_URL=http://localhost:6333
VECTOR_DB_COLLECTION=aegis_docs

# Transport Configuration
TRANSPORT_TYPE=in_memory  # in_memory | redis
REDIS_URL=

# HIL Configuration
HIL_API_URL=http://localhost:8001
HIL_DASHBOARD_URL=http://localhost:8501

# External API Keys (Replace with actual keys in production)
CHARGEBEE_API_KEY=PLACEHOLDER_CHARGEBEE_KEY
ZENDESK_API_KEY=PLACEHOLDER_ZENDESK_KEY
SUPPORT_API_KEY=PLACEHOLDER_SUPPORT_KEY

# Jira Configuration (for MCP integration)
# Your Jira instance URL (e.g., https://yourorg.atlassian.net)
JIRA_URL=

# Your Jira username/email
JIRA_USERNAME=

# Your Jira API token (generate from Atlassian account settings)
JIRA_API_TOKEN= 

# Default Jira project key (e.g., KAN, PROJ, DEV)
JIRA_PROJECT_KEY= 

# Jira base URL for creating links (e.g., https://yourorg.atlassian.net/browse)
JIRA_BASE_URL=

# Jira MCP Server Configuration (usually don't need to change)
JIRA_MCP_COMMAND=docker
JIRA_MCP_IMAGE=

# Chargebee MCP Configuration
CHARGEBEE_MCP_URL=

# Google ADK Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=

# ADK Agent Card Server (usually don't need to change)
ADK_AGENT_CARD_PORT=8080
ADK_SESSION_STORE=in_memory
